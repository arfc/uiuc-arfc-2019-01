	month = nov,
	year = {2014},
	keywords = {Fuel cycle, Gen IV, Molten salt reactor, Neutronic performance, Nuclear systems},
	pages = {308--319},
	file = {ScienceDirect Full Text PDF:/Users/khuff/Zotero/storage/WWISZQDL/Serp et al. - 2014 - The molten salt reactor (MSR) in generation IV Ov.pdf:application/pdf;ScienceDirect Full Text PDF:/Users/khuff/Zotero/storage/Q5WDR3ER/Serp et al. - 2014 - The molten salt reactor (MSR) in generation IV Ov.pdf:application/pdf;ScienceDirect Full Text PDF:/Users/khuff/Zotero/storage/Q5UID2I9/Serp et al. - 2014 - The molten salt reactor (MSR) in generation IV Ov.pdf:application/pdf;ScienceDirect Snapshot:/Users/khuff/Zotero/storage/QGLMLNIW/S0149197014000456.html:text/html;ScienceDirect Snapshot:/Users/khuff/Zotero/storage/ZRQWCMGZ/S0149197014000456.html:text/html;ScienceDirect Snapshot:/Users/khuff/Zotero/storage/FABGBG8S/S0149197014000456.html:text/html}

	pages = {82--98},
	file = {ScienceDirect Full Text PDF:/Users/khuff/Zotero/storage/JIPBQTRD/Zanetti et al. - 2015 - A Geometric Multiscale modelling approach to the a.pdf:application/pdf;ScienceDirect Full Text PDF:/Users/khuff/Zotero/storage/ZDHUIY95/Zanetti et al. - 2015 - A Geometric Multiscale modelling approach to the a.pdf:application/pdf;ScienceDirect Snapshot:/Users/khuff/Zotero/storage/NTTJSHEN/S0149197015000487.html:text/html;ScienceDirect Snapshot:/Users/khuff/Zotero/storage/ZZT9I5TK/S0149197015000487.html:text/html}
